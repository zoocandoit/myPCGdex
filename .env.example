# Supabase (클라이언트에서 사용 - NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Site URL (OAuth callback에 사용)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Google OAuth (선택사항 - Supabase Dashboard에서 설정 후 사용)
NEXT_PUBLIC_GOOGLE_OAUTH_ENABLED=false

# AI API Keys (서버에서만 사용 - 클라이언트에 노출 금지)
OPENAI_API_KEY=your_openai_api_key
# 또는
ANTHROPIC_API_KEY=your_anthropic_api_key

# Pokemon TCG API (선택사항 - 무료 티어는 키 없이 사용 가능)
POKEMON_TCG_API_KEY=your_pokemon_tcg_api_key
